<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/imageButtonMeet"
        android:layout_width="253dp"
        android:layout_height="72dp"
        android:layout_marginStart="141dp"
        android:layout_marginTop="184dp"
        android:layout_marginEnd="246dp"
        android:contentDescription="Meet People"
        android:foreground="@drawable/meet_people"
        android:scaleType="fitCenter"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.274"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/meet_people"
        tools:srcCompat="@drawable/meet_people" />

    <ImageButton
        android:id="@+id/imageButtonKirbie"
        android:layout_width="253dp"
        android:layout_height="72dp"
        android:layout_marginStart="141dp"
        android:layout_marginTop="460dp"
        android:layout_marginEnd="246dp"
        android:contentDescription="Talk To Kirbie"
        android:foreground="@drawable/talk_to_kirbie"
        android:scaleType="fitCenter"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.275"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/talk_to_kirbie"
        tools:srcCompat="@drawable/talk_to_kirbie" />

    <ImageButton
        android:id="@+id/imageButtonRoute"
        android:layout_width="253dp"
        android:layout_height="72dp"
        android:layout_marginStart="141dp"
        android:layout_marginTop="320dp"
        android:layout_marginEnd="246dp"
        android:contentDescription="Route"
        android:foreground="@drawable/route"
        android:scaleType="fitCenter"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.275"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/route"
        tools:srcCompat="@drawable/route" />

</androidx.constraintlayout.widget.ConstraintLayout>